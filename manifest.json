{
  "name": "Open it later",
  "version": "0.0.51",
  "description": "When you click on links, they are opened in a tab at some random time in the future",
  "update_url": "https://fluiddb.fluidinfo.com/about/open-it-later/fluidinfo.com/chrome-update.xml",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons" : {
    "16" : "images/logo-16.png",
    "19" : "images/logo-19.png",
    "32" : "images/logo-32.png",
    "48" : "images/logo-48.png",
    "64" : "images/logo-64.png",
    "128" : "images/logo-128.png"
  },
  "browser_action": {
    "default_icon": "images/logo-19.png",
    "default_title": "Open it later"
  }
}
